{
  "name": "articus/container-interop-symfony-console",
  "type": "library",
  "description": "Symfony Console factory for PSR-11 containers",
  "license": "MIT",
  "version": "0.1",
  "require": {
    "php": "^7.1",
    "psr/container": "^1.0.0",
    "symfony/console": "^4.0|^5.0",
    "zendframework/zend-stdlib": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\ContainerInteropSymfonyConsole\\": "tests/unit"
    }
  },
  "require-dev": {
    "symfony/event-dispatcher": "^4.0|^5.0",
    "codeception/base": "^2.3"
  },
  "scripts": {
    "test": "codecept run --coverage-xml"
  }
}
