{
  "name": "articus/container-interop-symfony-console",
  "type": "library",
  "description": "Symfony Console factory for PSR-11 containers",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "psr/container": "^1.0.0",
    "symfony/console": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\": "spec/"
    }
  },
  "require-dev": {
    "phpspec/phpspec": "^5.1|^6.1",
    "friends-of-phpspec/phpspec-code-coverage": "*"
  }
}
